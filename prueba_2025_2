    "/v3.0/workbench/alerts": {
      "get": {
        "operationId": "Get_V3_0_Workbench_Alerts",
        "summary": "List Workbench alerts",
        "description": "Return information about workbench alerts that match the specified criteria in a paginated list.",
        "parameters": [
          {
            "name": "TMV1-Filter",
            "description": "Trend Micro filter expression (e.g., status eq 'Open' and severity eq 'high').",
            "in": "header",
            "schema": {
              "type": "string"
            },
            "required": false
          },
          {
            "name": "startDateTime",
            "description": "ISO 8601 UTC start timestamp.",
            "in": "query",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "endDateTime",
            "description": "ISO 8601 UTC end timestamp.",
            "in": "query",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "dateTimeTarget",
            "description": "The timestamp to be used for retrieving Workbench alert data.",
            "in": "query",
            "schema": {
              "type": "string",
              "enum": [
                "createdDateTime",
                "updatedDateTime",
                "firstInvestigatedDateTime"
              ]
            }
          },
          {
            "name": "orderBy",
            "description": "Fields to sort by (e.g., createdDateTime desc).",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A list of Workbench alerts."
          }
        }
      }
    },
